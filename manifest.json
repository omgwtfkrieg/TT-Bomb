    ],
    "persistent": true
  },
 "options_ui": {
    "page": "src/options_custom/index.html",
    "chrome_style": false
  },
  "permissions": [
	"background",
    "notifications",
	"storage",
    "tabs",
	"alarms",
	"http://*/",
	"http://km2.timetrak.com:85/*",
    "https://*/*",
	"http://*/*",
	"http://*.timetrak.com:85/*",
	"webNavigation"
  ]
}
