{
  "name": "Ï€-BOMB : App built for nothing.",
  "version": "1.0.9",
  "manifest_version": 2,
  "description": "The automation of your nothingness.",
  "homepage_url": "http://trololololololololololo.com/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
	  "src/options_custom/jquery-1.11.1.min.js",
	  "src/options_custom/js/moment.js",
	  "src/bg/background.js"
    ],
    "persistent": true
  },

 "options_ui": {
  
    "page": "src/options_custom/index.html",
   
    "chrome_style": false
 
  },
  
  "permissions": [
	"background",
    "notifications",
	"storage",
    "tabs",
	"alarms",
    "https://*/*",
	"http://*/*",
	"webNavigation"
  ]
}