{
  "name": "Ï€-MOAB : The Motherload.",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "The automation of your nothingness.",
  "homepage_url": "http://www.omgwtfkrieg.com/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon48.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
	  "src/options_custom/jquery-1.11.1.min.js",
